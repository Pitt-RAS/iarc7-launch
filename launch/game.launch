<launch>
    <arg name="sim" default="false" doc="true if we are running in the sim" />
    <node name="game" pkg="iarc7_abstract" type="game.py" />
    <param name="safety_bonds/game/form_bond" value="true" />

    <include file="$(find iarc7_launch)/launch/main.launch">
        <arg name="sim" value="$(arg sim)" />
    </include>

</launch>
